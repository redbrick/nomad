olcObjectClasses: ( 1.3.6.1.4.1.62920.2.1 NAME 'brickie' DESC 'Redbrick member object class' SUP top STRUCTURAL MUST ( uid $ cn $ mail $ altmail $ studentNo $ courseCode $ userPassword $ homeDirectory $ uidNumber $ gidNumber $ storageQuota $ loginShell ) MAY ( expiryDate $ gecos $ discord $ github $ minecraft $ DateOfBirth $ year $ sshPublicKey $ adminNotes ) )
olcObjectClasses: ( 1.3.6.1.4.1.62920.2.2 NAME 'guest' DESC 'Redbrick guest object class' SUP top STRUCTURAL MUST ( uid $ cn $ mail $ altmail ) MAY ( expiryDate $ userPassword $ sshPublicKey $ discord $ github $ minecraft ) )
olcObjectClasses: ( 1.3.6.1.4.1.62920.2.3 NAME 'redbrickPosix' DESC 'Redbrick Server Access object class' SUP top AUXILIARY MUST ( homeDirectory $ uidNumber $ gidNumber $ storageQuota $ loginShell $ userPassword ) MAY ( gecos $ sshPublicKey ) )
olcObjectClasses: ( 1.3.6.1.4.1.62920.2.4 NAME 'orgAccount' DESC 'Redbrick Organisation Account clubs/soc object class' SUP top STRUCTURAL MUST ( uid $ cn $ altmail ) MAY ( expiryDate $ userPassword $ sshPublicKey ) )
olcObjectClasses: ( 1.3.6.1.4.1.62920.2.5 NAME 'reserved' DESC 'Reserved entries to prevent registration' SUP top STRUCTURAL MUST ( uid $ cn ) MAY ( description ) )
olcObjectClasses: ( 1.3.6.1.4.1.62920.2.6 NAME 'service' DESC 'Reserved entries to prevent registration' SUP top STRUCTURAL MUST ( uid $ cn $ userPassword ) MAY ( description $ mail ) )
